{
    "name":"presentations.scheduled-tasks",
    "version":"1.0.0",
    "author":"Giancarlo Gomez <giancarlo.gomez@gmail.com>",
    "type":"mvc",
    "devDependencies":{
        "commandbox-dotenv":"*",
        "commandbox-cfconfig":"*",
        "commandbox-cfformat":"*"
    },
    "dependencies":{
        "coldbox":"^7",
        "route-visualizer":"^2.2.0+1",
        "scheduled-task-manager":"^1.1.1"
    },
    "installPaths":{
        "coldbox":"coldbox/"
    },
    "scripts" : {
        "start:boxlang" : "server start serverConfigFile=server-boxlang.json --debug",
        "start:acf" : "server start serverConfigFile=server-acf.json --debug",
        "start:lucee" : "server start serverConfigFile=server-lucee.json --debug",
        "logs:boxlang" : "server log serverConfigFile=server-boxlang.json --follow",
        "logs:acf" : "server log serverConfigFile=server-acf.json --follow",
        "logs:lucee" : "server log serverConfigFile=server-lucee.json --follow"
    }
}
